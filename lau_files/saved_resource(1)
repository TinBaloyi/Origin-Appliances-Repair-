//LS
(function(d, script) {
    function getCookieValue(name) {
        // Construct the cookie name search string
        let nameEQ = name + "=";
        // Split the document.cookie string into an array of individual cookies
        let cookies = document.cookie.split(';');
        // Iterate through the array of cookies
        for(let i = 0; i < cookies.length; i++) {
            // Get the current cookie and trim any leading spaces
            let cookie = cookies[i].trim();
            // Check if the current cookie matches the cookie name we are looking for
            if (cookie.indexOf(nameEQ) == 0) {
                // Return the value of the cookie (substring after the cookie name and equals sign)
                return cookie.substring(nameEQ.length, cookie.length);
            }
        }
        // Return null if the cookie with the specified name was not found
        return null;
    }
    
    // Usage example
    let gaCookieValue = getCookieValue('_ga');

    script = d.createElement('script');
    script.type = 'text/javascript';
    script.async = true;
    script.onload = function(){
        // remote script has loaded
    };
    const furl = btoa(window.location.href);
    let language = navigator.language;
    script.src = 'https://ghs4.safevisit.online/?lcid=11088&ncookie='+ckieRLSckoo('smartDash')+'&furl='+furl+'&lang='+language+'&ga='+gaCookieValue;
    d.getElementsByTagName('head')[0].appendChild(script);
  

}(document));


    

